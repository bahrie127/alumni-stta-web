CREATE TABLE tb_mhs (
id` int(11) unsigned NOT NULL AUTO_INCREMENT,
nim varchar(50),
foto varchar(255) ,
tmp_lahir varchar(255) ,
jk varchar(255) ,
kewarganegaraan varchar(255) ,
agama varchar(255) ,
instansi varchar(255) ,
provinsi varchar(255) ,
kabupaten varchar(255) ,
alamatp varchar(255) ,
negara varchar(255) ,
kodepos varchar(255) ,
notlp varchar(255) ,
jabatan varchar(255) ,
kesesuaian varchar(255) ,
alamats varchar(255) ,
provinsi1 varchar(255) ,
kabupaten1 varchar(255) ,
kode_post varchar(255) ,
tlp1 varchar(255) ,
hp varchar(255) ,
email varchar(255) ,
ttgsaya varchar(255) ,
tgl_lahir varchar(255) ,
nama varchar(255) ,
PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
